{
    "name": "laravel-enso/tutorialmanager",
    "description": "Tutorial management dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "tutorialmanager",
        "tutorial-manager",
        "bootstrap-tour"
    ],
    "homepage": "https://github.com/laravel-enso/TutorialManager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel-enso/core": "2.14.*",
        "laravel-enso/vuedatatable": "1.4.*",
        "laravel-enso/formbuilder": "2.7.*",
        "laravel-enso/helpers": "1.8.*",
        "laravel-enso/permissionmanager": "2.1.*",
        "laravel-enso/structuremanager": "2.2.*"
    },
    "require-dev": {
        "laravel-enso/testhelper": "1.4.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\TutorialManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\TutorialManager\\AppServiceProvider"
            ],
            "aliases": {}
        }
    }
}
